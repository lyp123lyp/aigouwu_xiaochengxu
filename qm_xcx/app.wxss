/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.contain {
  padding: 0 10rpx;
}

.bgWhite{
   background-color: #fff;
}
.bglight{
  background-color:#f9f9f9;
}
.bgF5{
     background-color: #f5f5f5;
}
.mt-15{
  margin-top: 30rpx;
}
.m15{
  margin: 30rpx 0;
}
.clear:after{
  clear:both;
  display:block;
  content: "";
  overflow: hidden;
}
.hide {
  display: none;
}
.mb-15{
  margin-bottom:30rpx;
}
.m10{
  margin: 20rpx 0; 
}
.p5{
  padding: 10rpx;
}
.flex{
  display: flex;
  justify-content: space-between;
  position: relative;
}
.more{
  text-align: center;
  font-size: 14px;
}
.mt10{
  margin-top: 20rpx;
}
.plr10{
    padding:0 20rpx;
}
.mb8{
  margin-bottom: 16rpx;
}
.pb10{
  padding-bottom:20rpx; 
}

/* 超出两行隐藏 */
.webkitBox {
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
}