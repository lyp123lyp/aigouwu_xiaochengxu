  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.linethrough{
  text-decoration: line-through;
}
.contain {
  padding: 0 20rpx;
}
.hlike{
   color: #EC5151;
}
.bgWhite{
   background-color: #fff;
}
.bglight{
  background-color:#f9f9f9;
}
.bgF5{
     background-color: #f5f5f5;
}
.mt-15{
  margin-top: 30rpx;
}
.m15{
  margin: 30rpx 0;
}
.clear:after{
  clear:both;
  display:block;
  content: "";
  overflow: hidden;
}
.hide {
  display: none;
}
