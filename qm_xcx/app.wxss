}
.submit,
.closeBtn{
  width: 80%;
  background:#EC5151;
  height: 86rpx;
  margin:0;
  color: #fff;
  font-size: 18px;
  line-height: 86rpx;
}
.pd10{padding: 20rpx;}
/* 超出两行隐藏 */
.webkitBox {
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  text-overflow: ellipsis;
}
